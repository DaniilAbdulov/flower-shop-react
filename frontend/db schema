-- users {
-- id int pk increments unique >* orders.users_id
-- first_name varchar
-- last_name varchar
-- nickname varchar
-- email varchar
-- password varchar
-- created_at datetime
-- updated_at datetime
-- role enum def(USER)
-- }
-- category {

-- id int pk increments >* product.category_id
-- name varchar
-- }
-- product {

-- id int pk increments >* cart.product_id
-- title varchar
-- description varchar
-- price int
-- available int
-- img varchar
-- category_id int
-- }
-- status_orders {

-- id integer pk increments
-- status varchar
-- }
-- favorites {

-- id int pk increments
-- users_id int > users.id
-- product_id int >* product.id
-- }
-- trends {

-- id int pk increments
-- product_id int >* product.id
-- }
-- advice {

-- id int pk increments
-- product_id int >* product.id
-- }
-- orders {

-- id integer pk increments
-- users_id integer
-- date_order timestamp
-- status_order_id integer def(Создан) >* status_orders.id
-- total_price integer
-- }
-- carts_users {

-- users_id integer pk increments *> users.id
-- product_id integer *> product.id
-- count smallint
-- }
-- orders_products {

-- order_id integer pk increments *> orders.id
-- product_id integer *> product.id
-- count smallint
-- }

CREATE TABLE users ( id SERIAL PRIMARY KEY, first_name VARCHAR, last_name VARCHAR, nickname VARCHAR, email VARCHAR, password VARCHAR, created_at TIMESTAMPTZ, updated_at TIMESTAMPTZ, role VARCHAR DEFAULT 'USER' );
CREATE TABLE category ( id SERIAL PRIMARY KEY, name VARCHAR );
CREATE TABLE product ( id SERIAL PRIMARY KEY, title VARCHAR, description VARCHAR, price INT, available INT, img VARCHAR, category_id INTEGER );
CREATE TABLE status_orders ( id SERIAL PRIMARY KEY, status VARCHAR );
CREATE TABLE favorites ( id SERIAL PRIMARY KEY, users_id INTEGER REFERENCES users(id), product_id INTEGER REFERENCES product(id) );
CREATE TABLE trends ( id SERIAL PRIMARY KEY, product_id INTEGER REFERENCES product(id) );
CREATE TABLE advice ( id SERIAL PRIMARY KEY, product_id INTEGER REFERENCES product(id) );
CREATE TABLE orders ( id SERIAL PRIMARY KEY, users_id INTEGER REFERENCES users(id), date_order TIMESTAMPTZ, status_order_id INTEGER REFERENCES status_orders(id), total_price INTEGER );
CREATE TABLE carts_users ( users_id INTEGER REFERENCES users(id), product_id INTEGER REFERENCES product(id), count SMALLINT, PRIMARY KEY (users_id, product_id) );
CREATE TABLE orders_products ( order_id INTEGER REFERENCES orders(id), product_id INTEGER REFERENCES product(id), count SMALLINT, PRIMARY KEY (order_id, product_id) );

